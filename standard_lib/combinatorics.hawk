function factorial(x) {
    if x <= 1 return 1
    else return x * factorial(x - 1)
}

function permutation(x, y) {
    if x < y return -1
    else return factorial(x) / factorial(x - y)
}

function combination(x, y) {
    if x < y return -1
    else return factorial(x) / (factorial(x - y) * factorial(y))
}
