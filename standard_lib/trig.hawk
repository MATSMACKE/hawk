function cos(x) {
    return sin(x + pi()/2)
}

function tan(x) {
    return sin(x) / cos(x)
}

function csc(x) {
    return 1 / sin(x)
}

function sec(x) {
    return 1 / cos(x)
}

function cot(x) {
    return 1 / tan(x)
}

function sinh(x) {
    return (eNum(x) - eNum(-x))/2
}

function cosh(x) {
    return (eNum(x) + eNum(-x))/2
}

function tanh(x) {
    return sinh(x)/cosh(x)
}